{
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "amd": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow" : ["_id", "_message"]
        }
      ]
    }
}
